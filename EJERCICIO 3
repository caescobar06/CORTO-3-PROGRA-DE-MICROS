#include <stdio.h>
int main() {
 int i,n;
 int x=0, y=1,z;
 
 
 printf("Ingrese el l√≠mite de la serie fibonnaci ");
 scanf("%d", &n);
 
 for (i = 1; i <= n; i++) {
  z = x+y;//se obtiene el valor de la serie
 printf("El resultado es: %d \n", z);
  x = y; // X obtiene el valor de Y
  y = z; // Y obtiene el valor de la serie 
 }
 return 0;
}
